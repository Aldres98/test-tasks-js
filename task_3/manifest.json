{
  "name": "E-consulting highlight text",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Highlights everything that ends with -ing",
  "browser_action": {
    "default_popup": "ext.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab", "declarativeContent", "storage", "<all_urls>"],
  "manifest_version": 2
}
